  "scripts": {
    "start": "webpack-dev-server",
    "precommit": "lint-staged",
    "lint": "npm run csslint && npm run eslint",
    "csslint": "stylelint \"source/client/**/*.pcss\" --fix",
    "eslint": "eslint --ext .js,.jsx . --fix",
    "test": "jest",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors",
    "docs": "docsify serve ./docs -o -p 6789",
    "copy": "node scripts/deploy-copy",
    "upload": "cd deploy && s3-upload",
    "deploy": "npm run build:dist && npm run copy && npm run upload",
    "new:component": "hygen component new",
    "new:page": "hygen page new",
    "prettify": "prettier \"source/**/*.jsx\" --no-semi --single-quote --arrow-parens always --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,pcss,md}": [
      "npm run prettify",
      "git add"
    ]
  },
